<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.buaa.jj.matrixcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MatrixManagerActivity"
            android:label="@string/title_activity_matrix_manager"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".ApplicationMainService"
            android:enabled="true"
            android:exported="true">

        </service>

        <activity
            android:name=".MatrixCalculatorActivity"
            android:label="@string/title_activity_matrix_calculator"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".MatrixAnalyseActivity"
            android:label="Matrix Analyse">

        </activity>
    </application>

</manifest>